# Example .env for MultiCamSyncServer
# Copy this to .env and edit values as needed.

# Network
HOST_IP=0.0.0.0
CONTROL_PORT=5000
CAM1_PORT=5001
CAM2_PORT=5002

# Security token used by clients
AUTH_TOKEN=123456

# Data directories (relative to project root)
DATA_DIR=data
CAM1_SUBDIR=cam1
CAM2_SUBDIR=cam2

# Maximum frames per camera (0 = unlimited)
MAX_FRAMES=100
